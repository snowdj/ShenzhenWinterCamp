    "import pandas as pd\n",
    "import numpy as np\n",
    "import statsmodels.api as sm\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "t2= pd.read_csv(open('I:/ShenzhenWinterCamp/data-master/trump-world-trust/TRUMPWORLD-issue-1.csv',encoding='utf-8'))\n",
    "t3= pd.read_csv(open('I:/ShenzhenWinterCamp/data-master/trump-world-trust/TRUMPWORLD-issue-2.csv',encoding='utf-8'))\n",
    "t4= pd.read_csv(open('I:/ShenzhenWinterCamp/data-master/trump-world-trust/TRUMPWORLD-issue-3.csv',encoding='utf-8'))\n",
    "t5= pd.read_csv(open('I:/ShenzhenWinterCamp/data-master/trump-world-trust/TRUMPWORLD-issue-4.csv',encoding='utf-8'))\n",
    "t6= pd.read_csv(open('I:/ShenzhenWinterCamp/data-master/trump-world-trust/TRUMPWORLD-issue-5.csv',encoding='utf-8'))\n",
    "\n",
    "t2.mean().plot(kind='bar')\n",
    "plt.title('World'+' view about Trump issue 1')\n",
    "plt.show()\n",
    "#issue 1:Withdraw support for international climate change agreements \n",
    "\n",
    "t3.mean().plot(kind='bar')\n",
    "plt.title('World'+' view about Trump issue 2')\n",
    "plt.show()\n",
    "#issue 2:Build a wall on the border between the U. S. and Mexico\n",
    "\n",
    "t4.mean().plot(kind='bar')\n",
    "plt.title('World'+' view about Trump issue 3')\n",
    "plt.show()\n",
    "#issue 3:Withdraw U.S. support from the Iran nuclear weapons agreement\n",
    "\n",
    "t5.mean().plot(kind='bar')\n",
    "plt.title('World'+' view about Trump issue 4')\n",
    "plt.show()\n",
    "#issue 4:Withdraw U.S. support for major trade agreements\n",
    "\n",
