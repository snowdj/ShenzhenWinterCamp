    "import pandas as pd\n",
    "import numpy as np\n",
    "import statsmodels.api as sm\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "y = pd.read_csv(open('I:/ShenzhenWinterCamp/data-master/trump-world-trust/TRUMPWORLD-pres.csv',encoding='utf-8'))\n",
    "y1=y[14:]\n",
    "\n",
    "y_array=np.array(y1)\n",
    "y_arrayt=y_array.transpose()\n",
    "y1=y_arrayt[2:]\n",
    "\n",
    "t1= pd.read_csv(open('I:/ShenzhenWinterCamp/data-master/trump-world-trust/TRUMPWORLD-us.csv',encoding='utf-8'))\n",
    "t1=t1[16:]\n",
    "t1_array=np.array(t1)\n",
    "t1_arrayt=t1_array.transpose()\n",
    "x1=t1_arrayt[2:]\n",
    "\n",
    "issue1=np.array(t2['net_approval'])\n",
    "issue2=np.array(t3['net_approval'])\n",
    "issue3=np.array(t4['net_approval'])\n",
    "issue4=np.array(t5['net_approval'])\n",
    "issue5=np.array(t6['net_approval'])\n",
    "\n",
    "c1=np.ones(x1.shape)\n",
    "\n",
    "size_row,size_col=c1.shape\n",
    "x=np.zeros((size_row,4))\n",
    "\n",
    "for i in range(size_row):\n",
    "        x[i,0]=c1[i,0]\n",
    "        x[i,1]=x1[i,0]\n",
    "        x[i,2]=issue2[i]\n",
    "        x[i,3]=issue3[i]\n",
    "\n",
    "reg = sm.OLS(endog=y1,exog=x,missing='drop')\n",
    "results=reg.fit()\n",
    "print(results.summary())"
   ]
